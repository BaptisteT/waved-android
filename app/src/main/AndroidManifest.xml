<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.waved.streetshout.waved" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/WavedStyle" >
        <activity
            android:name="com.waved.streetshout.waved.activities.WelcomeActivity"
            android:label="@string/title_activity_welcome"
            android:theme="@style/WavedStyle.NoBar" >
>
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.waved.streetshout.waved.activities.PhoneNumberActivity"
            android:label="@string/title_activity_phone_number"
            android:theme="@style/WavedStyle.NoBar" >
        </activity>
    </application>

</manifest>
